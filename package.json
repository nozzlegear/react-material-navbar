{
  "name": "react-material-navbar",
  "version": "1.1.1",
  "description": "A simple React navbar inspired by Material UI, complete with full TypeScript definitions.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc && npm run webpack && npm run css",
    "css": "stylus -c --include-css -u autoprefixer-stylus ./css/all.styl -o ./dist",
    "css:watch": "stylus -c --include-css -u autoprefixer-stylus ./css/all.styl -o ./dist -w",
    "tsc": "tsc -p .",
    "webpack": "webpack dist/index.js dist/bundled.js && webpack dist/tests/test.js dist/tests/bundled.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nozzlegear/react-material-navbar.git"
  },
  "keywords": [
    "react",
    "winjs",
    "dialog",
    "windows",
    "uwp"
  ],
  "author": "Joshua Harms <joshua@nozzlegear.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nozzlegear/react-material-navbar/issues"
  },
  "homepage": "https://github.com/nozzlegear/react-material-navbar#readme",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.1.1"
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "autoprefixer-stylus": "^0.13.0",
    "react-dom": "^16.1.1",
    "react-icons": "^2.2.7",
    "stylus": "^0.54.5",
    "typescript": "^2.1.5",
    "utilities.styl": "^1.3.0",
    "webpack": "^2.2.1"
  }
}
